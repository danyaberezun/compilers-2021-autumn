-- Jump if False
public fun jmpF (label) {
  CJMP ("z", label)
}

-- Jump if True
public fun jmpT (label) {
  CJMP ("nz", label)
}

public fun jmpCond (c) {
  if compare (c, "z") == 0
  then fun (v) {v == 0}
  elif compare (c, "nz") == 0
  then fun (v) {v != 0}
  else failure ("Unknown jump condition")
  fi
}
